      "source": [
        "def jumlahList(nilai_list):\n",
        "    total = sum(nilai_list)\n",
        "    return total\n",
        "\n",
        "\n",
        "data = [10, 20, 30, 40]\n",
        "hasil = jumlahList(data)\n",
        "print(\"Total nilai:\", hasil)\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 2,
      "metadata": {
        "trusted": true
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "Nilai terbesar adalah: 31\n"
          ]
        }
      ],
      "source": [
        "def cariNilaiTerbesar(nilai_list):\n",
        "    terbesar = max(nilai_list)\n",
        "    return terbesar\n",
        "\n",
        "\n",
        "data = [15, 22, 8, 19, 31]\n",
        "hasil = cariNilaiTerbesar(data)\n",
        "print(\"Nilai terbesar adalah:\", hasil)\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 3,
      "metadata": {
        "trusted": true
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "Hasil penjumlahan: [5, 7, 9]\n"
          ]
        }
      ],
      "source": [
        "def jumlahkanList(list1, list2):\n",
        "    hasil = []\n",
        "    for i in range(len(list1)):\n",
        "        hasil.append(list1[i] + list2[i])\n",
        "    return hasil\n",
        "\n",
        "\n",
        "a = [1, 2, 3]\n",
        "b = [4, 5, 6]\n",
        "jumlah = jumlahkanList(a, b)\n",
        "print(\"Hasil penjumlahan:\", jumlah)\n"
      ]
    },
